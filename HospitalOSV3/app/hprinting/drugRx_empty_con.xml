	<queryString><![CDATA[SELECT  distinct t_visit.visit_hn                                          AS hn
	,t_visit.visit_vn                                          AS vn
	,t_patient.patient_pid                                     AS pid
	,t_patient.patient_xn				       AS xn
	,f_visit_type.visit_type_description                       AS patient_type
   	,b_contract_plans.contract_plans_description               AS plan
	,f_patient_prefix.patient_prefix_description|| ' '||t_patient.patient_firstname ||'  '|| t_patient.patient_lastname       AS name
    	,CURRENT_DATE  						       AS datevisit
     	,CURRENT_TIME 						       AS time
    	,t_visit.visit_dx                                          AS dx
	,t_visit.visit_patient_age                                 AS age
    	,b_visit_office_1.visit_office_name                        AS subhospital
	,b_visit_office.visit_office_name                          AS mainhospital
	,t_visit.visit_diagnosis_notice 			       AS dx_note
	,t_diag_icd10.diag_icd10_number                            AS diag_icd10
    	,b_icd10.icd10_description                                 AS disease_th     
    	,b_site.site_name   				       AS hospital
       
FROM t_visit 
	INNER JOIN t_patient t_patient ON t_patient.t_patient_id = t_visit.t_patient_id 
	LEFT JOIN t_diag_icd10 t_diag_icd10 ON (t_diag_icd10.diag_icd10_vn = t_visit.t_visit_id  and t_diag_icd10.f_diag_icd10_type_id ='1')
	LEFT JOIN b_icd10 b_icd10 ON t_diag_icd10.diag_icd10_number = b_icd10.icd10_number 
	left JOIN t_visit_payment t_visit_payment ON (t_visit_payment.t_visit_id = t_visit.t_visit_id and visit_payment_priority = '0')
	INNER JOIN b_contract_plans b_contract_plans ON t_visit_payment.b_contract_plans_id = b_contract_plans.b_contract_plans_id 
	LEFT JOIN b_visit_office b_visit_office ON b_visit_office.b_visit_office_id = t_visit_payment.visit_payment_main_hospital 
	LEFT JOIN b_visit_office b_visit_office_1 ON b_visit_office_1.b_visit_office_id = t_visit_payment.visit_payment_sub_hospital 
	LEFT JOIN  f_patient_prefix f_patient_prefix ON f_patient_prefix.f_patient_prefix_id = t_patient.f_patient_prefix_id																				
	INNER JOIN f_visit_type f_visit_type ON t_visit.f_visit_type_id = f_visit_type.f_visit_type_id 
	CROSS JOIN b_site b_site 

where t_visit.t_visit_id =  $P{visit_id}
]]></queryString>

