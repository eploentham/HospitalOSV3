,f_patient_marriage_status.patient_marriage_status_description as marry
,f_patient_occupation.patient_occupation_description as occupation
,f_patient_religion.patient_religion_description as religion
,f_patient_nation.patient_nation_description as nation
,race.patient_nation_description as race
,f_patient_prefix.patient_prefix_description 
 || ' ' || t_patient.patient_firstname 
 || ' ' || t_patient.patient_lastname as name
,b_contract_plans.contract_plans_description as plan
,t_patient.patient_hn as hn
,t_patient.patient_house 
 || ' หมู่ ' || t_patient.patient_moo
 || ' ถนน' || t_patient.patient_road 
 || ' ตำบล' || tambon.address_description
 || ' อำเภอ' || amphur.address_description 
 || ' จังหวัด' || changwat.address_description as address
,t_patient.patient_phone_number as telephonenumber
,t_patient.patient_contact_firstname
 || ' ' || t_patient.patient_contact_lastname as teller
,t_patient.patient_pid as pid
,t_patient.patient_house 
 || ' หมู่ ' || t_patient.patient_moo
 || ' ถนน' || t_patient.patient_road 
 || ' ตำบล' || tambon_ct.address_description
 || ' อำเภอ' || amphur_ct.address_description 
 || ' จังหวัด' || changwat_ct.address_description as telleraddress
,t_patient.patient_father_firstname
 || ' ' || t_patient.patient_father_lastname as fathername
,t_patient.patient_mother_firstname
 || ' ' || t_patient.patient_mother_lastname as mothername
,t_patient_personal_disease.patient_personal_disease_description as personaldisease
,t_patient.patient_update_date_time as date
,t_patient_payment.patient_payment_card_number as plancode
,t_patient_payment.patient_payment_card_issue_date as startplan
,t_patient_payment.patient_payment_card_expire_date as expplan
,f_patient_relation.patient_relation_description as relation
from t_patient 

left join f_patient_marriage_status on f_patient_marriage_status.f_patient_marriage_status_id = t_patient.f_patient_marriage_status_id
left join f_patient_occupation on f_patient_occupation.f_patient_occupation_id = t_patient.f_patient_occupation_id
left join f_patient_religion on f_patient_religion.f_patient_religion_id = t_patient.f_patient_religion_id
left join f_patient_nation on f_patient_nation.f_patient_nation_id = t_patient.f_patient_nation_id
left join (select * from f_patient_nation) as race on race.f_patient_nation_id = t_patient.f_patient_race_id
left join t_patient_payment on t_patient_payment.t_patient_id = t_patient.t_patient_id 
left join b_contract_plans on b_contract_plans.b_contract_plans_id = t_patient_payment.b_contact_plans_id
left join f_patient_prefix on t_patient.f_patient_prefix_id = f_patient_prefix.f_patient_prefix_id
left join (select * from f_address) as changwat on changwat.f_address_id = t_patient.patient_changwat
left join (select * from f_address) as amphur on amphur.f_address_id = t_patient.patient_amphur
left join (select * from f_address) as tambon on tambon.f_address_id = t_patient.patient_tambon
left join (select * from f_address) as changwat_ct on changwat_ct.f_address_id = t_patient.patient_changwat
left join (select * from f_address) as amphur_ct on amphur_ct.f_address_id = t_patient.patient_amphur
left join (select * from f_address) as tambon_ct on tambon_ct.f_address_id = t_patient.patient_tambon
left join t_patient_personal_disease on t_patient_personal_disease.t_patient_id = t_patient.t_patient_id
left join f_patient_relation on f_patient_relation.f_patient_relation_id = t_patient.f_patient_relation_id
where t_patient.t_patient_id = $P{patient_id}
--where t_patient.t_patient_id = '206000000000003031']]></queryString>

