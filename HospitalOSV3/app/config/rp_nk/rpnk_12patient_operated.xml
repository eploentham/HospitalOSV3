	</parameter>
	<queryString><![CDATA[SELECT 
query1.service_point 				AS servicepoint_name 
,r_operating_item.operating_item_description 	AS operating_item 
,sum(CASE WHEN (query1.sex = '1') 
		THEN 1 
			ELSE 0 
				END  ) 		AS male 
,sum(CASE WHEN (query1.sex = '2') 
		THEN 1 
			ELSE 0  
				END  ) 		AS female 

,sum(CASE WHEN (query1.sex <> '1' AND query1.sex <> '2') 
		THEN 1 
			ELSE 0 
				END  ) 		AS nonspec_sex
,count(query1.patient_id) 			AS total 
,query1.hospital_name    			AS hospital_name 

FROM r_operating_item
INNER JOIN (SELECT t_patient.t_patient_id  AS patient_id 
,b_item.b_item_id 			   AS item_id
,t_patient.f_sex_id     		   AS sex 
,b_service_point.service_point_description AS service_point 
,b_site.site_full_name                     AS hospital_name 
FROM  t_order INNER JOIN t_patient 
ON (t_order.t_patient_id = t_patient.t_patient_id 
AND t_order.f_order_status_id <> '0' 
AND t_order.f_order_status_id <> '1' 
AND t_order.f_order_status_id <> '3') 
INNER JOIN b_item 
ON b_item.b_item_id = t_order.b_item_id 
INNER JOIN b_employee 
ON b_employee.b_employee_id = t_order.order_staff_execute 
INNER JOIN b_service_point 
ON b_service_point. b_service_point_id = b_employee.b_service_point_id 
,b_site

WHERE  substr(t_order.order_executed_date_time,1,16) >= $P{datestart}   
          AND substr(t_order.order_executed_date_time,1,16) <= $P{dateend}

GROUP BY  t_patient.t_patient_id  
,b_item.b_item_id 
,t_patient.f_sex_id 
,b_service_point.service_point_description
,b_site.site_full_name    ) AS query1 
ON r_operating_item.b_item_id = query1.item_id 

GROUP BY query1.service_point 
,query1.hospital_name
,r_operating_item.operating_item_description 

ORDER BY query1.service_point]]></queryString>

