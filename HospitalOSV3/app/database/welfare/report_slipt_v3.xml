	<parameter name="t_visit_id" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA["255111770692848107"]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT f_patient_prefix.patient_prefix_description  AS prefix
    , t_patient.patient_firstname AS fname
    , t_patient.patient_lastname AS lname
    , t_visit.visit_patient_age AS age
    , t_visit.visit_hn AS hn
    , t_visit.visit_vn AS vn_an
    , CASE WHEN t_visit.f_visit_type_id = '0' 
        THEN 'ผู้ป่วยนอก'
        ELSE 'ผู้ป่วยใน'
      END AS patienttype
    , b_contract_plans.contract_plans_description AS payment
    , t_visit_payment.visit_payment_card_number AS payment_id
    , main.visit_office_name AS mainhospital
    , sub.visit_office_name AS subhospital
    , t_patient.patient_pid AS pid
    , t_patient.patient_phone_number AS telephonenumber 
    , t_patient.patient_house AS house
    , t_patient.patient_road AS road
    , t_patient.patient_moo AS moo
    , tambon.address_description AS tambon
    , amphur.address_description AS amphur
    , changwat.address_description AS changwat
    , t_visit.visit_begin_visit_time AS "date"
    , b_item.item_common_name AS detail
    , t_billing_invoice_item.billing_invoice_item_total as price
    , b_site.site_name AS hospital
FROM t_visit 
    INNER JOIN t_patient ON (t_visit.t_patient_id = t_patient.t_patient_id 
        AND t_visit.t_visit_id like $P{t_visit_id})
    INNER JOIN t_visit_payment 
        ON (t_visit.t_visit_id = t_visit_payment.t_visit_id 
            AND t_visit_payment.visit_payment_priority = '0')
    INNER JOIN
        (SELECT * FROM f_address WHERE f_address.address_tambon_type = '1') AS changwat 
        ON t_patient.patient_changwat = changwat.f_address_id
    INNER JOIN
        (SELECT * FROM f_address WHERE f_address.address_tambon_type = '2') AS amphur
        ON t_patient.patient_amphur= amphur.f_address_id
    INNER JOIN
        (SELECT * FROM f_address WHERE f_address.address_tambon_type = '3') AS tambon
        ON t_patient.patient_tambon = tambon.f_address_id
    INNER JOIN f_patient_prefix ON t_patient.f_patient_prefix_id = f_patient_prefix.f_patient_prefix_id    
    INNER JOIN
        (SELECT b_visit_office_id ,visit_office_name FROM b_visit_office) AS main
        ON t_visit_payment.visit_payment_main_hospital = main.b_visit_office_id
    INNER JOIN
        (SELECT b_visit_office_id ,visit_office_name FROM b_visit_office) AS sub
        ON t_visit_payment.visit_payment_sub_hospital = sub.b_visit_office_id
    INNER JOIN b_contract_plans ON t_visit_payment.b_contract_plans_id = b_contract_plans.b_contract_plans_id
    INNER JOIN t_order ON (t_visit.t_visit_id = t_order.t_visit_id AND t_order.order_complete = '1')
    INNER JOIN t_billing_invoice_item ON t_order.t_order_id = t_billing_invoice_item.t_order_item_id
--    INNER JOIN b_item_16_group ON t_order.b_item_16_group_id = b_item_16_group.b_item_16_group_id
    INNER JOIN b_item ON t_order.b_item_id = b_item.b_item_id
    , b_site
ORDER BY detail]]></queryString>

