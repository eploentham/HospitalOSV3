	<parameter name="t_visit_id" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["0470000020"]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT
	"patient"."prefix"             AS "prefix",
	"patient"."firstname"          AS "fname",
	"patient"."hn"		       AS "hn",
	"patient"."lastname"           AS "lname",
	"visit"."vn"                   AS "vn_an",
	 visit_type.name           AS "patienttype",
	"plan"."description"           AS "payment",
	"payment"."card_no"            AS "payment_id",
	"office"."off_name"            AS "mainhospital",
	"office_1"."off_name"          AS "subhospital",
	"patient"."pid"                AS "pid",
	"patient"."tel"                AS "telephonenumber",
	"patient"."address"            AS "house_road",
	"patient"."village"            AS "moo",
	"address_2"."text"             AS "amphur",
	"address"."text"               AS "tambon",
	"address_1"."text"             AS "changwat",
	"visit"."begin_visit_time"     AS "date",
	"item"."common_name"           AS "detail",
	"billing_item"."patient_share" AS "price",
	"site"."off_name"              AS "hospital",
	(CURRENT_DATE - "patient"."birthdate")/365          AS "age" 
FROM
	 "visit" "visit"
	left JOIN "payment" "payment"                     ON "payment"."vn" = "visit"."vn" 
	left JOIN "order_item" "order_item" ON "order_item"."vn" = "visit"."vn" 
	left JOIN "billing_item" "billing_item" ON "billing_item"."order_item_id" = "order_item"."order_item_id" 
	left JOIN "item" "item"               	ON "item"."item_code" = "order_item"."item_code" 
	left JOIN "patient" "patient"   ON "visit"."hn" = "patient"."hn" 
	left JOIN "address" "address_1"      ON "patient"."changwat"||'0000' = "address_1"."fullcode" 
	left JOIN "address" "address_2"   ON "address_2"."fullcode" = "patient"."changwat" || "patient"."amphur" ||'00'
	left JOIN "address" "address"         ON "patient"."changwat" || "patient"."amphur" || "patient"."district" =  "address"."fullcode" 
	left JOIN "office" 	"office"   ON "office"."off_code" = "payment".	"hosp_main" 
	left JOIN "office" "office_1"   ON "payment"."hosp_sub" = "office_1"	."off_code" 
	left JOIN "plan" 	"plan"              	ON "payment"."plan_code" = 	"plan"."plan_code" 
	left join visit_type on visit_type.code = visit.visit_type
	CROSS JOIN "site" 	"site" 
WHERE
	("visit"."vn" =$P{t_visit_id})]]></queryString>

