        SimpleDateFormat df = new SimpleDateFormat("dd/MM/yyyy");
        if(showTime)
            df = new SimpleDateFormat("dd/MM/yyyy hh:mm");
        Date dated = DateUtil.getDateFromText(date);
        if(dated==null)
            return "";
        System.out.println(df.format(dated));
        return df.format(dated);
    }

