	<queryString><![CDATA[Select 
	b_site.site_full_name                                         as hospital_name
	,t_patient.patient_hn as hn
	,substring(t_billing_invoice.t_billing_invoice_date_time,12,5) as billing_time
	,f_patient_prefix.patient_prefix_description || t_patient.patient_firstname || ' ' || t_patient.patient_lastname as fullname
	,b_rpnk_map_plan.rpnk_map_plan_ref_id as debt
	,t_billing_invoice.billing_invoice_total as remain
	,b_rpnk_map_plan.rpnk_map_plan_ref_id as plan
	,t_visit.f_visit_type_id as visit_type

From 
	b_contract_plans inner join t_visit_payment on (b_contract_plans.b_contract_plans_id = t_visit_payment.b_contract_plans_id)
	inner join b_rpnk_map_plan on (b_rpnk_map_plan.rpnk_map_plan_std_id = b_contract_plans.b_contract_plans_id)
	inner join t_visit on (t_visit.t_visit_id = t_visit_payment.t_visit_id
		and t_visit_payment.visit_payment_active <> '0'
		and t_visit_payment.visit_payment_priority ='0'	
		)
	inner join t_billing_invoice on (t_billing_invoice.t_visit_id = t_visit.t_visit_id		
		and t_billing_invoice.billing_invoice_active <> '0'		
		and substr(t_billing_invoice.t_billing_invoice_date_time,1,16) >= $P{datestart}    	
		and substr(t_billing_invoice.t_billing_invoice_date_time,1,16) <= $P{dateend}
		and t_billing_invoice.billing_invoice_total <> '0'
		) 	
	inner join t_patient on (t_visit.t_patient_id = t_patient.t_patient_id		
		and (t_visit.f_visit_status_id = '2' or t_visit.f_visit_status_id = '3'))	
	left join f_patient_prefix on (f_patient_prefix.f_patient_prefix_id = t_patient.f_patient_prefix_id)
	, b_site
where
	b_rpnk_map_plan.rpnk_map_plan_ref_id = '06' or
	b_rpnk_map_plan.rpnk_map_plan_ref_id = '07' or
	b_rpnk_map_plan.rpnk_map_plan_ref_id = '08' or
	b_rpnk_map_plan.rpnk_map_plan_ref_id = '09' or
	b_rpnk_map_plan.rpnk_map_plan_ref_id = '10' or
	b_rpnk_map_plan.rpnk_map_plan_ref_id = '11'
Order By
	t_billing_invoice.t_billing_invoice_date_time]]></queryString>

