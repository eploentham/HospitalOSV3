package com.report18file.gui;

import com.report18file.control.*;
import com.hospital_os.usecase.connection.ConnectionInf;
import com.hosv3.utility.connection.UpdateStatus;
import com.report18file.objdb.EpiDB;
import com.report18file.objdb.Rp18DB;
import com.report18file.object.*;

/**
 *
 * @author henbe
 */
public class DBSuitS extends DBSuit{

    public DBSuitS(UpdateStatus us,ConnectionInf con){
        theConnectionInf = con;
         init18FileControl(us);
    }

    public static DBSuit getDBSuit(UpdateStatus us,ConnectionInf con){
        return new DBSuitS(us,con);
    }
    private void init18FileControl(UpdateStatus us) {
        theAncDB = new Rp18DB(theConnectionInf,new Anc());
        theAppointDB = new Rp18DB(theConnectionInf,new Appoint());
        theCardDB = new Rp18DB(theConnectionInf,new Card());
        theChronicDB = new Rp18DB(theConnectionInf,new Chronic());
        theDeathDB = new Rp18DB(theConnectionInf,new Death());
        theDiagDB = new Rp18DB(theConnectionInf,new Diag());
        theDrugDB = new Rp18DB(theConnectionInf,new Drug());
        theEpiDB = new EpiDB(theConnectionInf,new Epi());
        theFpDB = new Rp18DB(theConnectionInf,new Fp());
        theHomeDB = new Rp18DB(theConnectionInf,new Home());
        Mch theMCH = new Mch();
        theMCH.setConnectionInf(theConnectionInf);
        thePpDB = new Rp18DB(theConnectionInf,theMCH);
        Pp thePP = new Pp();
        thePP.setConnectionInf(theConnectionInf);
        thePpDB = new Rp18DB(theConnectionInf,thePP);
        theNutritionDB = new Rp18DB(theConnectionInf,new Nutrition());
        thePersonDB = new Rp18DB(theConnectionInf,new Person());
        theProcedDB = new Rp18DB(theConnectionInf,new Proced());
        theServeilDB = new Rp18DB(theConnectionInf,new Serveil());
        theServiceDB = new Rp18DB(theConnectionInf,new Service());
        theWomanDB = new Rp18DB(theConnectionInf,new Woman());
    }
}
