package com.pcu.object;

import com.hospital_os.usecase.connection.Persistent;

/**
 *
 * @author LionHeart
 */
public class MchDetail extends Persistent{
    String initString = "";
    public String gravida = initString;
    public String alive_baby = initString;
    public String death_baby = initString;
    public String born_place = initString;
    public String last_men_date = initString;
    public String birth_date = initString;
    public String birth_method = initString;
    public String bddoctor = initString;
    public String born_result = initString;
    public String born_at_hospital = initString;
    public String vdrl_rs = initString;
    public String hb_rs = initString;
    public String hiv_rs = initString;
    public String hct_date = initString;
    public String thalassaemia = initString;//2.
    public String dental = initString;
    public String decayed_tooth = initString;
    public String limestone = initString;
    public String inflame_gum = initString;
    public String hct_lavel = initString;
    public String t_health_family_id = initString;
    public String active = initString;
    public String born_amount = initString;
    public void setObject(BornMch bornMch,Pregnancy pregnancy,AncPcu ancPcu)
    {
        if(bornMch==null)
            bornMch = new BornMch();
        gravida = bornMch.gravida;
        alive_baby = bornMch.lborn;
        death_baby = bornMch.sborn;
        born_place = bornMch.bplace;
        bddoctor = bornMch.bdoctor;
        birth_method = bornMch.birthmethod;
        born_result = bornMch.bresult;
        born_at_hospital = bornMch.bhosp;

        if(pregnancy == null)
            pregnancy = new Pregnancy();
        last_men_date = pregnancy.pregnancy_menses_issue_date;
        birth_date = pregnancy.pregnancy_menses_expire_date;
        if(ancPcu == null)
            ancPcu = new AncPcu();
        vdrl_rs = ancPcu.anc_vdrl;
        hb_rs = ancPcu.anc_hb;
        hiv_rs = ancPcu.anc_hiv;
        thalassaemia = ancPcu.anc_thalassemia;
        inflame_gum = ancPcu.anc_dental_gum;
        limestone = ancPcu.anc_dental_tartar;
        hct_date = ancPcu.anc_hct_date;
        hct_lavel = ancPcu.anc_hct_result;
    }
}
